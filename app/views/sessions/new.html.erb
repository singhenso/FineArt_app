<h1>Log In</h1>

<%= form_for :login, url: login_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit 'Log in' %>
    </div>
  <% end %>

  <%= link_to "sign up", new_user_path %>









